{
  "name": "common-ui-components",
  "version": "1.0.9",
  "description": "Made with create-react-library",
  "author": "igalleibovich",
  "license": "MIT",
  "repository": "igalleibovich/common-ui-components",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rimraf dist && microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment --css-modules false && cpx src/assets/** dist/assets && cpx src/**/*.css dist/",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
